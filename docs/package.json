{
  "name": "docs",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack --hostname 0.0.0.0",
    "dev1": "next dev --turbopack --hostname 0.0.0.0 --port 3001",
    "prebuild": "pnpm --filter ritoswap run build-sb:docs",
    "build": "next build",
    "start": "next start --hostname 0.0.0.0",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "postbuild": "node scripts/generate-sitemap.mjs && pagefind --site .next/server/app --output-path public/_pagefind",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@mdx-js/react": "^3.1.0",
    "@theguild/remark-mermaid": "^0.3.0",
    "howler": "^2.2.4",
    "next": "^15.5.9",
    "nextra": "^4.0.0",
    "nextra-theme-docs": "^4.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^6.0.0",
    "swagger-ui-react": "^5.27.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0",
    "@next/eslint-plugin-next": "^15.5.4",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/howler": "^2.2.12",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.0.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.0.0",
    "@types/swagger-ui-react": "^5.18.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "9.37.0",
    "eslint-config-next": "13.4.4",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "globals": "^16.4.0",
    "jsdom": "^24.1.0",
    "pagefind": "^1.2.0",
    "postcss-preset-env": "^10.2.4",
    "typescript": "^5.5.4",
    "vitest": "^2.1.3"
  }
