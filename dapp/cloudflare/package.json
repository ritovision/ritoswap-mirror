{
  "name": "cloudflare",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "secret:state": "wrangler secret put STATE_SERVICE_AUTH_TOKEN",
    "secret:brevo": "wrangler secret put BREVO_API_KEY",
    "secret:sender": "wrangler secret put SENDER_EMAIL",
    "secret:receiver": "wrangler secret put RECEIVER_EMAIL",
    "secret:all": "pnpm secret:brevo && pnpm secret:sender && pnpm secret:receiver && pnpm secret:state",
    "test": "vitest",
    "check": "tsc --noEmit"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241205.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "wrangler": "^4.47.0"
  }
}
