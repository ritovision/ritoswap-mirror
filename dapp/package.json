{
  "name": "ritoswap",
  "version": "1.0.0",
  "packageManager": "pnpm@10.13.1",
  "type": "module",
  "private": true,
  "scripts": {
    "--- Site Scripts ---": ":",
    "postinstall": "prisma generate --schema=./prisma/schema.prisma",
    "prebuild": "pnpm codegen:tools && pnpm codegen:presenters",
    "dev": "next dev --hostname 0.0.0.0",
    "dev:ritonet": "cross-env NEXT_PUBLIC_ACTIVE_CHAIN=Ritonet next dev --hostname 0.0.0.0",
    "dev:sepolia": "cross-env NEXT_PUBLIC_ACTIVE_CHAIN=Sepolia next dev --hostname 0.0.0.0",
    "dev:ethereum": "cross-env NEXT_PUBLIC_ACTIVE_CHAIN=Ethereum next dev --hostname 0.0.0.0",
    "build": "npx prisma generate && next build",
    "build:ritonet": "cross-env NEXT_PUBLIC_ACTIVE_CHAIN=Ritonet next build",
    "build:sepolia": "cross-env NEXT_PUBLIC_ACTIVE_CHAIN=Sepolia next build",
    "build:ethereum": "cross-env NEXT_PUBLIC_ACTIVE_CHAIN=Ethereum next build",
    "build:pwa": "cross-env NODE_ENV=production next build",
    "start": "next start",
    "generate:openapi": "tsx scripts/generate-openapi.ts --pretty",
    "postbuild": "pnpm run generate:openapi",
    "dev:openapi": "tsx scripts/generate-openapi.ts --pretty --server http://localhost:3000",
    "lint": "eslint .",
    "lint:errors": "pnpm exec eslint . --ext .js,.jsx,.ts,.tsx --quiet",
    "lint:fix": "eslint . --fix",
    "secret:jwt": "node -e \"console.log(require('crypto').randomBytes(64).toString('base64url'))\"",
    "typecheck": "tsc --noEmit",
    "--- Vitest Scripts ---": ":",
    "test": "vitest",
    "test:contract": "vitest run app/api/__contract__/contract.test.ts",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "--- Playwright e2e Scripts ---": ":",
    "build:wallet-inject": "esbuild e2e/playwright/wallet/injected/provider.ts --bundle --outfile=e2e/playwright/wallet/injected-provider.js --platform=browser --target=es2019 --format=iife --sourcemap --minify",
    "dev:wallet-inject": "pnpm run build:wallet-inject -- --watch",
    "test:e2e:setup": "node e2e/setup.js",
    "test:e2e": "playwright test",
    "test:e2e:hp": "playwright test dapp/e2e/playwright/hp-tokengate.spec.ts --project=chrome --workers=1",
    "test:e2e:mintburn": "playwright test dapp/e2e/playwright/hp-mint-burn.spec.ts --project=chrome --workers=1",
    "test:e2e:wallet": "playwright test dapp/e2e/playwright/wallet.spec.ts",
    "test:e2e:swap": "playwright test dapp/e2e/playwright/swap-widget.spec.ts",
    "test:e2e:assets": "playwright test dapp/e2e/playwright/portfolio-assets.spec.ts --project=chrome --workers=1",
    "test:e2e:smoke": "playwright test dapp/e2e/playwright/smoke.spec.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:dev": "playwright test --project=dev-server",
    "test:e2e:prod": "pnpm run build && playwright test --project=prod-build",
    "test:e2e:install": "playwright install --with-deps",
    "test:e2e:report": "playwright show-report",
    "test:e2e:clean": "rm -rf test-results playwright-report",
    "test:e2e:ci": "pnpm run test:e2e:setup && pnpm run test:e2e:prod",
    "test:e2e:quick": "playwright test --project=chromium",
    "test:e2e:build": "pnpm run build",
    "test:e2e:start": "pnpm run start",
    "test:e2e:prod:all": "playwright test --timeout=60000",
    "test:all": "pnpm run test && pnpm run test:e2e",
    "--- Supertest e2e Scripts ---": ":",
    "supertest:e2e": "vitest run --config ./e2e/supertest/vitest.config.supertest.ts",
    "supertest:e2e:watch": "vitest --config ./e2e/supertest/vitest.config.supertest.ts",
    "supertest:e2e:local": "TEST_BASE_URL=http://localhost:3000 pnpm supertest:e2e",
    "supertest:e2e:testnet": "TEST_BASE_URL=https://testnet.ritoswap.com pnpm supertest:e2e",
    "supertest:e2e:mainnet": "TEST_BASE_URL=https://ritoswap.com pnpm supertest:e2e",
    "supertest:e2e:state-on": "NEXT_PUBLIC_ENABLE_STATE_WORKER=true pnpm supertest:e2e",
    "supertest:e2e:state-off": "NEXT_PUBLIC_ENABLE_STATE_WORKER=false pnpm supertest:e2e",
    "supertest:e2e:full": "pnpm supertest:e2e:state-on && pnpm supertest:e2e:state-off",
    "supertest:e2e:coverage": "vitest run --config ./e2e/supertest/vitest.config.supertest.ts --coverage",
    "--- Postman e2e Scripts ---": ":",
    "postman:fetch": "tsx postman/scripts/fetch-openapi.ts",
    "postman:convert": "openapi2postmanv2 -s postman/collection/openapi.json -o postman/collection/collection.json -p -O folderStrategy=Tags",
    "postman:jwt-capture": "tsx postman/scripts/add-jwt-capture-test.ts",
    "postman:jwt": "tsx postman/scripts/add-jwt-requests.ts",
    "postman:presign": "tsx postman/scripts/presign-requests.ts",
    "postman:sign": "tsx postman/scripts/add-signing-to-collection.ts",
    "postman:tests": "tsx postman/scripts/add-tests-to-collection.ts",
    "postman:run": "newman run postman/collection/collection.json -e postman/collection/local.postman_environment.json --delay-request 100",
    "postman:report": "newman run postman/collection/collection.json -e postman/collection/local.postman_environment.json -r htmlextra --reporter-htmlextra-export postman/collection/report.html",
    "postman:all": "pnpm postman:fetch && pnpm postman:convert && pnpm postman:sign && pnpm postman:tests && pnpm postman:jwt-capture && pnpm postman:jwt && pnpm postman:presign && pnpm postman:run",
    "--- AI System Scripts ---": ":",
    "ai:testprovider": "pnpm exec tsx app/lib/llm/__tests__/__provider-tests__/test-provider.ts",
    "ai:checkmodel": "pnpm exec tsx app/lib/llm/__tests__/__provider-tests__/debug-provider.ts",
    "quota:token:reset:all": "pnpm tsx scripts/quotas/quota-token-reset.ts --all",
    "quota:token:reset:ids": "cross-env QUOTA_TOKEN_IDS=123,456 pnpm tsx scripts/quotas/quota-token-reset.ts",
    "quota:crypto:reset:all": "pnpm tsx scripts/quotas/quota-crypto-reset.ts --all",
    "quota:crypto:reset:addresses": "cross-env QUOTA_CRYPTO_ADDRESSES=0xabc...,0xdef... pnpm tsx scripts/quotas/quota-crypto-reset.ts",
    "codegen:tools": "tsx scripts/codegen/generate-tool-types.ts",
    "codegen:presenters": "tsx scripts/codegen/generate-presenters-index.ts",
    "prisma:reset": "tsx scripts/PrismaReset.ts",
    "--- Storybook Scripts ---": ":",
    "sb": "storybook dev -p 6006",
    "build-sb": "storybook build",
    "build-sb:docs": "storybook build -o ../docs/public/storybook-static"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.23",
    "@ai-sdk/provider-utils": "^3.0.7",
    "@ai-sdk/react": "^2.0.28",
    "@asteasolutions/zod-to-openapi": "^8.1.0",
    "@aws-sdk/client-s3": "^3.846.0",
    "@aws-sdk/s3-request-presigner": "^3.846.0",
    "@langchain/cloudflare": "^0.1.1",
    "@langchain/community": "^0.3.54",
    "@langchain/core": "^0.3.73",
    "@langchain/openai": "^0.6.11",
    "@lifi/widget": "^3.33.1",
    "@pinecone-database/pinecone": "^6.1.2",
    "@prisma/client": "^6.10.1",
    "@prisma/extension-accelerate": "^2.0.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-tabs": "^1.1.13",
    "@sentry/nextjs": "10",
    "@tanstack/react-query": "^5.83.0",
    "@wagmi/connectors": "5.8.4",
    "ai": "^5.0.28",
    "cross-env": "^7.0.3",
    "dompurify": "^3.2.6",
    "ethers": "5.8.0",
    "framer-motion": "^12.18.1",
    "fuse.js": "^7.1.0",
    "howler": "^2.2.4",
    "jose": "^6.1.0",
    "langchain": "^0.3.32",
    "next": "^14.2.35",
    "next-pwa": "^5.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.5.2",
    "react-qr-code": "^2.0.18",
    "react-swipeable": "^7.0.2",
    "siwe": "^3.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.27.1",
    "tslib": "^2.8.1",
    "viem": "^2.31.2",
    "wagmi": "2.17.5",
    "winston": "^3.17.0",
    "zod": "^4.0.17",
    "zod-to-json-schema": "^3.24.6",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0",
    "@next/eslint-plugin-next": "^14.2.35",
    "@playwright/test": "^1.53.2",
    "@storybook/addon-docs": "9.1.16",
    "@storybook/nextjs-vite": "9.1.16",
    "@storybook/react": "9.1.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/howler": "^2.2.12",
    "@types/node": "20.2.5",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-react": "^5.18.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "dotenv": "^17.2.0",
    "encoding": "^0.1.13",
    "esbuild": "^0.25.10",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.35",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-storybook": "9.1.16",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.4.0",
    "happy-dom": "^18.0.1",
    "msw": "^2.10.3",
    "newman": "^6.2.1",
    "newman-reporter-htmlextra": "^1.23.1",
    "openapi-to-postmanv2": "^5.0.1",
    "openapi-types": "^12.1.3",
    "pino-pretty": "^13.1.2",
    "prisma": "^6.10.1",
    "server-only": "^0.0.1",
    "storybook": "9.1.16",
    "supertest": "^7.1.4",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^5.4.19",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}