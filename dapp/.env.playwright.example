# Copy this file to `.env.playwright` inside the `dapp/` directory and fill in the values below.
# These are the only variables consumed by `dapp/e2e/playwright/env.ts` and the CI workflow.

# Test wallet private key (0x-prefixed; use a throwaway Sepolia key)
PRIVATE_KEY=

# Network + chain configuration
# Options: Ethereum, Sepolia, Ritonet (custom). CHAIN_ID is required when using a custom network.
NETWORK=Sepolia
CHAIN_ID=11155111

# RPC endpoint to use for the injected wallet (optional; falls back to public Sepolia RPCs)
RPC_URL=https://rpc.sepolia.org

# Base URL the tests should visit (CI passes its preview URL via TEST_BASE_URL/BASE_URL)
TEST_BASE_URL=http://localhost:3000

# Optional overrides
WALLET_NAME=Test Wallet
TX_DELAY_MS=2000
MCP_ENDPOINT=/api/mcp
