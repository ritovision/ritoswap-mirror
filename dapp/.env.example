#Prisma Postgres for tracking Colored Keys usage status
DATABASE_URL="your-database-url-for-tracking-colored-keys-usage-status"

# Cloudflare State worker (durable object)
NEXT_PUBLIC_ENABLE_STATE_WORKER=true
STATE_WORKER_API_KEY=
STATE_WORKER_URL=

# Active Chain Configuration -> Ethereum, Sepolia, Ritonet
NEXT_PUBLIC_ACTIVE_CHAIN=Sepolia

#RitoNet Localhost
NEXT_PUBLIC_LOCAL_CHAIN_ID=90999999
NEXT_PUBLIC_LOCAL_BLOCKCHAIN_NAME=RitoNet
NEXT_PUBLIC_LOCAL_BLOCKCHAIN_EXPLORER_URL=
NEXT_PUBLIC_LOCAL_BLOCKCHAIN_EXPLORER_NAME=RitoNet Explorer
NEXT_PUBLIC_LOCAL_BLOCKCHAIN_RPC=http://localhost:8545
NEXT_PUBLIC_LOCAL_BLOCKCHAIN_WSS=ws://localhost:8546

#RPC Endpoints
NEXT_PUBLIC_ALCHEMY_API_KEY=

#WalletConnect
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=
NEXT_PUBLIC_APP_NAME=App Name
NEXT_PUBLIC_APP_DESCRIPTION=App Description


#Email Configurations
BREVO_API_KEY=
SENDER_EMAIL=
RECEIVER_EMAIL=
USE_CLOUDFLARE_WORKER=false
CLOUDFLARE_WORKER_URL=

#Cloudflare R2
R2_API_ACCESS_KEY_ID=
R2_API_SECRET_ACCESS_KEY=
R2_API_BUCKET_NAME=
R2_API_ACCOUNT_ID=

#Service Worker for PWA
NEXT_PUBLIC_SW=true

# AI Provider for LLMs (openai | lmstudio)
AI_PROVIDER=
AI_CHAT_MAX_OUTPUT_TOKENS=
AI_CHAT_MAX_DURATION=
NEXT_PUBLIC_AI_CHAT_REQUIRES_JWT=true
NEXT_PUBLIC_AI_CHAT_API_PATH=/api/chat
## (0 or 1)
DEBUG_STREAMING=
DEBUG_TOOLS=

#OpenAI
OPENAI_API_KEY=
AI_TEMPERATURE=
AI_BASE_URL=http://127.0.0.1:1234
AI_OPENAI_MODEL_1=gpt-5.1
AI_OPENAI_MODEL_2=gpt-4
AI_OPENAI_MODEL_3=
AI_OPENAI_VISION_MODEL=gpt-4o-mini

#LM Studio
AI_LOCAL_MODEL_1=
AI_LOCAL_MODEL_2=
AI_LOCAL_MODEL_3=
AI_LOCAL_VISION_MODEL=

# AI Image Generation
AI_IMAGE_PROVIDER=openai
OPENAI_IMAGE_MODEL=gpt-image-1.5
IMAGE_DEFAULT_SIZE=1024x1024
IMAGE_DEFAULT_QUALITY=low
IMAGE_MAX_CONCURRENCY=1

# Text-to-speech
## Set provider to "elevenlabs" to enable
TTS_PROVIDER=disabled
ELEVENLABS_API_KEY=
ELEVENLABS_VOICE_ID=
ELEVENLABS_MODEL_ID=
ELEVENLABS_BASE_URL=
ELEVENLABS_OUTPUT_FORMAT=mp3_44100_128
ELEVENLABS_VOICE_STABILITY=
ELEVENLABS_VOICE_SIMILARITY=
NEXT_PUBLIC_TTS_API_PATH=/api/tts

# Pinecone Vector Database
PINECONE_API_KEY=
PINECONE_INDEXES=
PINECONE_CLOUD=
PINECONE_REGION=
## Index 1 Configuration
PINECONE_INDEX_1_NAME=
## Can be comma-separated: namespace1,namespace2,namespace3
PINECONE_INDEX_1_NAMESPACES=

# MCP Configuration
MCP_ENABLED=true                    
MCP_ENDPOINT=/api/mcp  

# AI Quotas (for chat tokens and crypto transfers)
AI_CHAT_QUOTA_ENABLED=true
AI_CHAT_QUOTA_TOKENS=10000
AI_CHAT_QUOTA_WINDOW_SEC=1000
AI_QUOTA_RESET_SECRET=
RESET_API_URL=
AI_PRIVATE_KEY=
## Master switch (on by default). Set to false to disable tracking.
AI_CRYPTO_QUOTA=false
## Global daily spend cap (ETH) for the active network.
## Example: 1.5 = max 1.5 ETH total per 24h.
AI_CRYPTO_QUOTA_DAILY_LIMIT=.3
## Per-user spend cap (ETH) for the active network.
## Example: 0.3 = max 0.3 ETH per address per 24h.
AI_CRYPTO_QUOTA_USER_LIMIT=0.1
## Window duration in seconds (default 86400 = 24h).
## Applies to both global and per-user limits.
AI_CRYPTO_QUOTA_DURATION=86400


# Notification Controls
# Set to "false" to disable specific notification types
# Any other value or empty/missing will enable notifications by default
NEXT_PUBLIC_LOCAL_NOTIFICATIONS=

# Logger config
# debug | info | warn | error
LOG_LEVEL=debug  
SERVICE_NAME=
ENABLE_FILE_LOGS=
SHOW_TEST_LOGS=


# Backdoor configuration for api/form-submission-gateway using backdoorToken (for testing purposes)
# Setting this allows a specific token to be automatically reset after being successfully marked as "used" in the prisma database.
# A private key is needed to sign the request in either of the other test env files.
BACKDOOR_TOKEN=false
TOKEN_ID=
# Locks in a specific address to be used for the backdoor token, preventing others from using it even if they have the token.
BACKDOOR_ADDRESS=

VERCEL_ENV=development


#JWT (use pnpm secret:jwt to generate a secret)
JWT_ALG=HS256
JWT_SECRET=
JWT_AUD=localhost:3000
JWT_ACCESS_TTL=900
JWT_CLOCK_TOLERANCE=5

# Sentry
# Client DSN (public)
NEXT_PUBLIC_SENTRY_DSN=
# Optional server override (fallbacks to NEXT_PUBLIC_SENTRY_DSN if unset)
SENTRY_DSN=
# Optional environment tag (e.g. development | preview | production | mainnet | sepolia)
SENTRY_ENVIRONMENT=
# Optional source map upload (build-time only)
SENTRY_AUTH_TOKEN=
SENTRY_ORG=
SENTRY_PROJECT=
SENTRY_RELEASE=
